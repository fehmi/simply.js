var t0 = performance.now();
function simpleScope(scope) {
  return function (el) {
    if (el.type !== 'rule') return;

    el.props = el.props.map(p => {

      // :global(...) escape
      if (p.includes(':global(')) {
        return p.replace(/:global\((.*?)\)/g, '$1');
      }

      // @keyframes iÃ§i selector'a dokunma
