      // Zaten scoped
      if (p.includes(scope)) return p;

      // Root selector (:host, :where, [attr])
      if (p[0] === ':' || p[0] === '[') {
        return `${p}${scope}`;
      }

      return `${scope} ${p}`;
    });
  };
}


